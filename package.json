{
  "name": "myaceroyal-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve -c production",
    "start:uat": "ng serve -c uat",
    "start:dev": "ng serve -c development",
    "start:netlify": "ng serve -c netlify",
    "build": "ng build",
    "build:prod": "ng build -c production",
    "build:uat": "ng build -c uat",
    "build:dev": "ng build -c development",
    "build:netlify": "ng build -c netlify",
    "build:github-pages": "ng build -c github-pages",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "netlify:deploy": "npm run build:netlify && netlify deploy --prod --dir=dist/myaceroyal-admin",
    "netlify:preview": "npm run build:netlify && netlify deploy --dir=dist/myaceroyal-admin",
    "vercel:deploy": "npm run build:prod && vercel --prod",
    "surge:deploy": "npm run build:prod && surge dist/myaceroyal-admin/ myaceroyal-admin.surge.sh",
    "serve": "serve dist/myaceroyal-admin -s -p 3000",
    "deploy": "git pull origin development && vercel --prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/angular-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "chart.js": "^4.5.0",
    "feather-icons": "^4.29.2",
    "ng-zorro-antd": "^19.3.1",
    "ngx-cookie-service": "^19.0.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.11.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "serve": "^14.2.1",
    "typescript": "~5.7.2"
  }
}
